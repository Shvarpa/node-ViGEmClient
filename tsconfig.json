{
	"compilerOptions": {
		// "incremental": true,
		"target": "es2019",
		"module": "commonjs",
		"allowJs": true,
		"checkJs": false,
		"jsx": "react",
		"outDir": "./lib",
		"sourceMap": true,
		"strict": false,
		"alwaysStrict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"rootDir": "./src"
	},
	"lib": ["esnext"],
	"include": ["src/**/*"],
	"exclude": ["node_modules", "example", "lib"]
}
